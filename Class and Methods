class Car:
    def __init__(self, brand: str, horsepower: int) ->None:
        self.brand = brand
        self.horsepower = horsepower

    def drive(self) -> None:
        print(f'{self.brand} is driving!')

    def get_info(self) -> None:
        print(f'{self.brand} with {self.horsepower} horsepower')


McLaren: Car = Car('McLaren',400)
McLaren.drive()
McLaren.get_info()

Porsche: Car = Car('McLaren',400)
Porsche.drive()
Porsche.get_info()

Lamborghini: Car = Car('McLaren',400)
Lamborghini.drive()
Lamborghini.get_info()
